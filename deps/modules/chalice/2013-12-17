#%Module1.0

set VERSION [file tail $ModulesCurrentModulefile]
set PACKAGE [file tail [file dirname $ModulesCurrentModulefile]]
set MOD_HOME [file dirname [file dirname [file dirname $ModulesCurrentModulefile]]]
set PLATFORM $tcl_platform(platform)
set LOC $MOD_HOME/$PACKAGE/$VERSION

conflict $PACKAGE

setenv [string toupper $PACKAGE]_HOME $LOC
prepend-path PATH $LOC/$PLATFORM/bin


